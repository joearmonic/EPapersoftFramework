<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="ioCSection" type="TunstallWindowsService.IoC.ConfigurationHandler.DIComponentSection, TunstallWindowsService.IoC, Version=1.0.0.0, Culture=neutral" requirePermission="false" />
    <section name="tunstallservice" type="TunstallWindowsService.ConfigurationHandler.TunstallServiceSection, TunstallWindowsService, Version=1.0.0.0, Culture=neutral" requirePermission="false" />
    <sectionGroup name="activities" type="TunstallWindowsService.ConfigurationHandler.ActivitiesSectionGroup, TunstallWindowsService, Version=1.0.0.0, Culture=neutral">
      <section name="activities.runtime" type="TunstallWindowsService.ConfigurationHandler.ActivitiesSettingSection, TunstallWindowsService, Version=1.0.0.0, Culture=neutral" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <appSettings>
  </appSettings>
  <ioCSection>
    <DIComponents>
      <component assemblyName="TunstallWindowsService.FileWriter" />
    </DIComponents>
  </ioCSection>
  <tunstallservice>
    <workers>
      <worker id="1" name="filewriter" factory="">
        <programming period="WeeklyInterval">
          <appointment hour="22" minute="12"/>
          <!--<until hour="23" minute="59"/>-->
        <!--<daysofweek>
          <dayofweek name="Monday"/>
        <dayofweek name="Tuesday"/>
        <dayofweek name="Thursday"/>
        </daysofweek>-->
        </programming>
      </worker>
    </workers>
  </tunstallservice>
  <activities>
    <activities.runtime>
      <activities.settings>
        <!-- Time in minutes to stop if stop is commanded and the app seems not to responds in that time -->
        <add key="OnStopWaitTimeout" value="3" />
        <add key="OnReloadWaitTimeout" value="1" />
      </activities.settings>
    </activities.runtime>
  </activities>
  <connectionStrings>
    
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.diagnostics configSource="Tracing.config"></system.diagnostics>
</configuration>
